{
	"width": 640,
	"height": 360,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"assets/font64.png": {
			"type": "image",
			"path": "assets/font64.png",
			"width": 512,
			"height": 512
		},
		"assets/card64.png": {
			"type": "image",
			"path": "assets/card64.png",
			"width": 256,
			"height": 256
		},
		"util": {
			"type": "script",
			"path": "script/util.js",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"ranking"
			]
		},
		"preferredSessionParameters": {
			"totalTimeLimit": 65
		}
	},
	"globalScripts": []
}